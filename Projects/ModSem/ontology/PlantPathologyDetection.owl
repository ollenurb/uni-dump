    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember">
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection"/>
        <rdfs:comment xml:lang="en">A relation between collections and entities, e.g. &apos;my collection of saxophones includes an old Adolphe Sax original alto&apos; (i.e. my collection has member an Adolphe Sax alto).</rdfs:comment>
        <rdfs:comment xml:lang="en">The link to the members of a Set</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl"/>
        <rdfs:label xml:lang="en">hasMember</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf">
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection"/>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl"/>
        <rdfs:label xml:lang="en">isMemberOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#PlantPart"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Plant"/>
        <rdfs:comment xml:lang="en">A transitive relation expressing parthood between any entities, e.g. &apos;brain is a part of the human body&apos;. When specializing this pattern, take care of restricting the domain and range appropriately, since it could be counterintuitive to use this relation arbitrarily, e.g. between animals and planets. For an intransitive partof pattern, see &apos;componency&apos;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl"/>
        <rdfs:label xml:lang="it">eParteDi</rdfs:label>
        <rdfs:label xml:lang="en">isPartOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#affectedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#affectedBy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/onto_plant_pathology#affects"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Plant"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Pathogen"/>
        <rdfs:comment xml:lang="it">La pianta viene affetta da un patogeno.</rdfs:comment>
        <rdfs:comment xml:lang="en">The plant being affected by a pathogen.</rdfs:comment>
        <rdfs:label xml:lang="en">affected by</rdfs:label>
        <rdfs:label xml:lang="it">affetto da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#affects -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#affects">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Pathogen"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Plant"/>
        <rdfs:comment xml:lang="it">Il patogeno affligge una pianta.</rdfs:comment>
        <rdfs:comment xml:lang="en">The pathogen affecting a plant.</rdfs:comment>
        <rdfs:label xml:lang="en">affects</rdfs:label>
        <rdfs:label xml:lang="it">affligge</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#canDevelop -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#canDevelop">
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/onto_plant_pathology#affectedBy"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/onto_plant_pathology#causesSymptom"/>
        </owl:propertyChainAxiom>
        <rdfs:label xml:lang="en">can develop</rdfs:label>
        <rdfs:label xml:lang="it">può sviluppare</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#causedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#causedBy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/onto_plant_pathology#causes"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#CommonSymptoms"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Pathogen"/>
        <rdfs:comment xml:lang="it">L&apos; insieme di sintomi comuni è causato da un patogeno.</rdfs:comment>
        <rdfs:comment xml:lang="en">The set of Common Symptoms is caused by a Pathogen.</rdfs:comment>
        <rdfs:label xml:lang="it">causati da</rdfs:label>
        <rdfs:label xml:lang="en">caused by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#causes -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#causes">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Pathogen"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#CommonSymptoms"/>
        <rdfs:comment xml:lang="it">Il patogeno causa un insieme di sintomi comuni.</rdfs:comment>
        <rdfs:comment xml:lang="en">The pathogen causes a set of Common Symptoms.</rdfs:comment>
        <rdfs:label xml:lang="it">causa</rdfs:label>
        <rdfs:label xml:lang="en">causes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#causesSymptom -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#causesSymptom">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Pathogen"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#DiseaseSymptom"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/onto_plant_pathology#causes"/>
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Property chain used to list which symptom is caused by a pathogen (bypassing the Common Symptoms Entity).</rdfs:comment>
        <rdfs:comment xml:lang="it">Property chain utilizzata per elencare quale sintomo è causato dal patogeno (bypassando l&apos;entità Common Symptoms)</rdfs:comment>
        <rdfs:label xml:lang="it">causa sintomo</rdfs:label>
        <rdfs:label xml:lang="en">causes Symptom</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#concernedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#concernedBy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/onto_plant_pathology#concerns"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#PlantPart"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#DiseaseSymptom"/>
        <rdfs:label xml:lang="en">concerned by</rdfs:label>
        <rdfs:label xml:lang="it">riguardato da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#concerns -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#concerns">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#DiseaseSymptom"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#PlantPart"/>
        <rdfs:label xml:lang="en">concerns</rdfs:label>
        <rdfs:label xml:lang="it">riguarda</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#hasSubnationalTerritories -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#hasSubnationalTerritories">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Nation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#SubnationalTerritories"/>
        <rdfs:label xml:lang="it">ha territori subnazionali</rdfs:label>
        <rdfs:label xml:lang="en">has Subnational Territories</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#hasSubterritoryMember -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#hasSubterritoryMember">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/onto_plant_pathology#isSubterritoryMember"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#SubnationalTerritories"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#SubnationalTerritory"/>
        <rdfs:label xml:lang="it">ha membro sottoterritorio</rdfs:label>
        <rdfs:label xml:lang="en">has Subterritory Member</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#hasSymptomMember -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#hasSymptomMember">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#hasMember"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/onto_plant_pathology#isSymptomMember"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#CommonSymptoms"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#DiseaseSymptom"/>
        <rdfs:label xml:lang="it">ha membro sintomo</rdfs:label>
        <rdfs:label xml:lang="en">has Symptom Member</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#isSubterritoryMember -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#isSubterritoryMember">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#SubnationalTerritory"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#SubnationalTerritories"/>
        <rdfs:label xml:lang="en">is Subterritory Member</rdfs:label>
        <rdfs:label xml:lang="it">è membro sottoterritorio</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#isSymptomMember -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#isSymptomMember">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#DiseaseSymptom"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#CommonSymptoms"/>
        <rdfs:label xml:lang="en">is Symptom Member</rdfs:label>
        <rdfs:label xml:lang="it">è membro sintomo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#presenceIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#presenceIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Presence"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#GeographicalEntity"/>
        <rdfs:comment xml:lang="it">La partecipazione di un&apos;entità geografica all&apos;interno di una relazione di presenza patogeno-territorio.</rdfs:comment>
        <rdfs:comment xml:lang="en">The partecipation of a territory in a pathogen-territory presence relationship.</rdfs:comment>
        <rdfs:label xml:lang="en">presence in</rdfs:label>
        <rdfs:label xml:lang="it">presenza in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#presenceOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#presenceOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Presence"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Pathogen"/>
        <rdfs:comment xml:lang="it">La partecipazione di un patogeno all&apos;interno di una relazione di presenza patogeno-territorio.</rdfs:comment>
        <rdfs:comment xml:lang="en">The partecipation of a pathogen in a pathogen-territory presence relationship.</rdfs:comment>
        <rdfs:label xml:lang="en">presence of</rdfs:label>
        <rdfs:label xml:lang="it">presenza di</rdfs:label>
    </owl:ObjectProperty>
    


