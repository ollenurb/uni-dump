            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/onto_plant_pathology#Plant"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/onto_plant_pathology#affectedBy"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.semanticweb.org/onto_plant_pathology#DeadlyPathogen"/>
                                    <rdf:Description rdf:about="http://www.semanticweb.org/onto_plant_pathology#HighlyInfectivePathogen"/>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Plant"/>
        <rdfs:comment xml:lang="en">A plant affected by a pathogen that is both highly infective and deadly.</rdfs:comment>
        <rdfs:comment xml:lang="it">Una pianta affetta da un patogeno sia altamente infettivo sia mortale.</rdfs:comment>
        <rdfs:label xml:lang="en">Endangered Plant</rdfs:label>
        <rdfs:label xml:lang="it">Pianta in pericolo</rdfs:label>
    </owl:Class>
    


