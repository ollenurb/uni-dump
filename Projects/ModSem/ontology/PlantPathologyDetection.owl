    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->

    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri"/>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#EPPOCode -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#EPPOCode">
        <rdfs:comment xml:lang="it">Il codice identificativo dell&apos;European and Mediterranean Plant Protection Organization (EPPO) Global Database.</rdfs:comment>
        <rdfs:comment xml:lang="en">The identifying code of the European and Mediterranean Plant Protection Organization (EPPO) Global Database.</rdfs:comment>
