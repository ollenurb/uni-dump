    <!-- http://www.semanticweb.org/onto_plant_pathology#hasAuthority -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#hasAuthority">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#BiologicalEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">La persona o le persone che hanno pubblicato per prime il nome tassonomico dell&apos;entità biologica.</rdfs:comment>
        <rdfs:comment xml:lang="en">The scientist or scientists who first validly published the taxonomical name of the biological entity.</rdfs:comment>
        <rdfs:label xml:lang="it">ha autorità</rdfs:label>
        <rdfs:label xml:lang="en">has Authority</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#hasCommonName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#hasCommonName">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#BiologicalEntity"/>
        <rdfs:comment xml:lang="it">Il nome comune con cui è nota un&apos;entità biologica.</rdfs:comment>
        <rdfs:comment xml:lang="en">The common name by which a Biological Entity is known.</rdfs:comment>
        <rdfs:label xml:lang="it">ha nome comune</rdfs:label>
        <rdfs:label xml:lang="en">has Common Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#hasEppoCode -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#hasEppoCode">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#BiologicalEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Il codice identificativo dell&apos;European and Mediterranean Plant Protection Organization (EPPO) Global Database associato all&apos;entità biologica.</rdfs:comment>
        <rdfs:comment xml:lang="en">The identifying code of the European and Mediterranean Plant Protection Organization (EPPO) Global Database associated to the Biological Entity.</rdfs:comment>
        <rdfs:label xml:lang="it">ha codice EPPO</rdfs:label>
        <rdfs:label xml:lang="en">has EPPO Code</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#hasEradicatedPathogen -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#hasEradicatedPathogen">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Presence"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="it">L&apos;eradicazione di un patogeno in un dato territorio.</rdfs:comment>
        <rdfs:comment xml:lang="en">The eradication of a pathogen in a given territory.</rdfs:comment>
        <rdfs:label xml:lang="it">ha patogeno eradicato</rdfs:label>
        <rdfs:label xml:lang="en">has Eradicated Pathogen</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#hasFirstEncounter -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#hasFirstEncounter">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Presence"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedShort"/>
        <rdfs:comment xml:lang="it">L&apos;anno di prima rilevazione di un patogeno in un territorio.</rdfs:comment>
        <rdfs:comment xml:lang="en">The first encounter year of a pathogen in a territory.</rdfs:comment>
        <rdfs:label xml:lang="it">ha prima rilevazione</rdfs:label>
        <rdfs:label xml:lang="en">has First Encounter</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#hasISO_3166_2_Code -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#hasISO_3166_2_Code">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#SubnationalTerritory"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Il codice ISO per l&apos;identificazione dei territori nazionali e subnazionali.</rdfs:comment>
        <rdfs:comment xml:lang="en">The ISO code for the identification of national and subnational territories.</rdfs:comment>
        <rdfs:label xml:lang="it">ha codice ISO 3166:2</rdfs:label>
        <rdfs:label xml:lang="en">has ISO 3166:2 Code</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/onto_plant_pathology#hasScientificName -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/onto_plant_pathology#hasScientificName">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/onto_plant_pathology#BiologicalEntity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="it">Il nome scientifico con cui è nota un&apos;entità biologica.</rdfs:comment>
        <rdfs:comment xml:lang="en">The scientific name by which a biological entity is known.</rdfs:comment>
