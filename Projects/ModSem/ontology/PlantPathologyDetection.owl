    <!-- http://www.semanticweb.org/onto_plant_pathology#DeadlyPathogen -->

    <owl:Class rdf:about="http://www.semanticweb.org/onto_plant_pathology#DeadlyPathogen">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/onto_plant_pathology#Pathogen"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/onto_plant_pathology#causesSymptom"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Death"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/onto_plant_pathology#Pathogen"/>
        <rdfs:comment xml:lang="en">Any pathogen which can eventually cause the death of its host organism.</rdfs:comment>
        <rdfs:comment xml:lang="it">Qualsiasi patogeno che può eventualmente causare la morte del suo organismo ospitante.</rdfs:comment>
        <rdfs:label xml:lang="en">Deadly Pathogen</rdfs:label>
