     xmlns:onto_plant_pathology="http://www.semanticweb.org/onto_plant_pathology#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/onto_plant_pathology">
        <dc:creator>Brunello Matteo</dc:creator>
        <dc:creator>Caresio Lorenzo</dc:creator>
        <dc:date>2024-02</dc:date>
        <dc:description xml:lang="en">A resource to represent and support the identification of pathogens and diseases of plants.</dc:description>
        <dc:description xml:lang="it">Risorsa per la rappresentazione e il supporto dell&apos;identificazione di patogeni e malattie di piante.</dc:description>
        <dc:title>PathoPlants Ontology</dc:title>
        <vann:preferredNamespacePrefix rdf:resource="http://www.semanticweb.org/onto_plant_pathology"/>
        <vann:preferredNamespaceUri rdf:resource="http://www.semanticweb.org/onto_plant_pathology"/>
        <rdfs:label>PathoPlants</rdfs:label>
    </owl:Ontology>
    


